<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Controle</servlet-name>
        <servlet-class>Controle.Superviseur</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Geonote</servlet-name>
        <servlet-class>Geonote.Geonote</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controle</servlet-name>
        <url-pattern>/addPath</url-pattern>
        <url-pattern>/register</url-pattern>
        <url-pattern>/index</url-pattern>
        <url-pattern>/profile</url-pattern>
        <url-pattern>/addnote</url-pattern>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Geonote</servlet-name>
        <url-pattern>/AddPath</url-pattern>
        <url-pattern>/Profile</url-pattern>
        <url-pattern>/Index</url-pattern>
        <url-pattern>/Login</url-pattern>
        <url-pattern>/Addnote</url-pattern>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
